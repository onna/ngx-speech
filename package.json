    "tslint": "~5.12.1",
    "typescript": "~3.1.6",
    "webpack": "4.19.1"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
